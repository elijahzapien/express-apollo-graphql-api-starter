{
  "name": "express-apollo-graphql-api-starter",
  "version": "0.0.0",
  "main": "server.js",
  "scripts": {
    "dependency-validation:nsp": "nsp check --reporter table",
    "dependency-validation:snyk": "env-cmd .env snyk test",
    "dependency-validation": "npm run dependency-validation:nsp && npm run dependency-validation:snyk",
    "set-development": "cross-env NODE_ENV=development",
    "set-production": "cross-env NODE_ENV=production",
    "debug": "DEBUG='Server,Server:*'",
    "clean": "rimraf dist",
    "setup": "npm run debug node scripts/setup.js",
    "lint": "eslint . --ext .js,.es6",
    "compile": "node scripts/compile",
    "start": "npm run set-development && npm run debug nodemon src/server.js",
    "build": "npm run set-production && npm run clean && npm run compile",
    "production": "npm run build && node dist/server.js"
  },
  "repository": {},
  "author": "Elijah Zapien",
  "license": "MIT",
  "dependencies": {
    "apollo-server-express": "1.2.0",
    "bcrypt": "1.0.3",
    "body-parser": "1.18.2",
    "compression": "1.7.1",
    "dotenv": "4.0.0",
    "env-cmd": "7.0.0",
    "express": "4.16.2",
    "express-validator": "4.3.0",
    "graphql": "0.11.7",
    "graphql-tag": "2.5.0",
    "graphql-tools": "2.8.0",
    "helmet": "3.9.0",
    "jsonwebtoken": "8.1.0",
    "mongoose": "4.13.7",
    "morgan": "1.9.0",
    "nsp": "3.1.0",
    "snyk": "1.62.0",
    "uuid": "3.1.0",
    "validator": "9.2.0"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.2",
    "babel-loader": "7.1.2",
    "babel-plugin-module-resolver": "3.0.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-register": "6.26.0",
    "babel-runtime": "6.26.0",
    "chalk": "2.3.0",
    "cross-env": "5.1.1",
    "eslint": "4.11.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-babel": "4.1.2",
    "figures": "2.0.0",
    "nodemon": "1.12.1",
    "rimraf": "2.6.2"
  }
}
